{
  "name": "babel_webpack_starter",
  "version": "1.0.0",
  "description": "Starter pack for compiling ES6+ apps to ES5",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --output-public-path=/build/  --mode development --progress --open --hot",
    "build": "node ./node_modules/webpack/bin/webpack.js --mode production --progress",
    "json:server": "node ./node_modules/json-server/lib/cli/bin.js --watch api/db.json"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "json-server": "^0.16.1"
  }
}
